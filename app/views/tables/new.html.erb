<div class="row">
    <div  class="col s4 push-s4">
        <h2>Register a new table</h2>
        
        <%= form_for @table do |f| %>
            <p>
                <%= f.label :number %> <br/>
                <%= f.number_field :number, value: @last_table_number %>
            </p>
            <p>
                <%= f.submit "save", class: "waves-effect waves-light btn" %>
            </p>
        <%end%>
    </div>
</div>